<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>VoteSystemServer</display-name>
  <servlet>
    <servlet-name>doDelete</servlet-name>
    <servlet-class>com.sangebang.water.servlet.doDelete</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>doDelete</servlet-name>
    <url-pattern>/doDelete.do</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
  	<servlet-name>doAdd</servlet-name>
  	<servlet-class>com.sangebang.water.servlet.doAdd</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>doFindAll</servlet-name>
    <servlet-class>com.sangebang.water.servlet.doFindAll</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>doUpdate</servlet-name>
    <servlet-class>com.sangebang.water.servlet.doUpdate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>OnLogin</servlet-name>
    <servlet-class>com.sangebang.water.servlet.OnLogin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>doUpload</servlet-name>
    <servlet-class>com.sangebang.water.servlet.doUpload</servlet-class>
  </servlet>




  <servlet-mapping>
  	<servlet-name>doAdd</servlet-name>
  	<url-pattern>/doAdd.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>doFindAll</servlet-name>
    <url-pattern>/doFindAll.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>doUpdate</servlet-name>
    <url-pattern>/doUpdate.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OnLogin</servlet-name>
    <url-pattern>/OnLogin.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>doUpload</servlet-name>
    <url-pattern>/doUpload.do</url-pattern>
  </servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>



</web-app>